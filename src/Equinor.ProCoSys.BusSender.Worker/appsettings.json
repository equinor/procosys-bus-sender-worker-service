{
  "Logging": {
    "PathFormat": "c:/Logs/log-{Date}.txt",
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Equinor.ProCoSys.BusSender.Core": "Information"
    }
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  },
  "ConnectionString": "#{ProcosysDb}",
  "ServiceBusConnectionString": "#{ServiceBusConnectionString}",
  "MessageChunkSize": 30,
  "TopicNames": "commpkg,mcpkg,project",
  "TimerInterval": "#{TimerInterval}"
}
